{
  "name": "@opendex/auth-service",
  "version": "1.0.0",
  "description": "Enterprise-grade OAuth2/OIDC Authentication Microservice",
  "author": "OpenDex Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio --port 5556",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/config": "^3.4.0",
    "@nestjs/swagger": "^8.0.10",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.1",
    "@nestjs/terminus": "^11.1.2",
    "@nestjs/throttler": "^7.0.1",
    "@nestjs/event-emitter": "^2.1.0",
    "@nestjs/cqrs": "^11.0.2",
    "@prisma/client": "^6.12.0",
    "@prisma/instrumentation": "^6.12.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "joi": "^17.14.0",
    "ioredis": "^5.4.4",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "jose": "^5.2.2",
    "@oslojs/otp": "^1.1.0",
    "@simplewebauthn/server": "^11.0.0",
    "@node-oauth/oauth2-server": "^5.1.0",
    "oidc-provider": "^8.5.1",
    "openid-client": "5.6.4",
    "helmet": "^8.0.0",
    "compression": "^1.7.4",
    "@sentry/node": "^8.48.0",
    "@sentry/profiling-node": "^8.48.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.56.0",
    "prom-client": "^15.1.4",
    "rxjs": "^7.8.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.4",
    "@nestjs/testing": "^11.1.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-oauth2": "^1.4.18",
    "@types/bcrypt": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "rimraf": "^5.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
